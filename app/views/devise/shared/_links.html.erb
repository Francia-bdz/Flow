<%- if controller_name != 'sessions' %>
<br/>
  <%= link_to "Déjà un compte ? Se connecter", new_session_path(resource_name), class: "a" %><br />
<% end %>



<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
<br/>
  <%= link_to "Pas de compte ? S'inscrire", new_registration_path(resource_name), class: "a"  %><br />
<% end %>


<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
<br/>
  <%= link_to "Mot de passe oublié?", new_password_path(resource_name), class: "a"  %><br />
<% end %>


<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
<br/>
  <%= link_to "Didn't receive confirmation instructions?", new_confirmation_path(resource_name), class: "a"  %><br />
<% end %>


<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <br/>
  <%= link_to "Didn't receive unlock instructions?", new_unlock_path(resource_name), class: "a"  %><br />
<% end %>


<%- if devise_mapping.omniauthable? %>
<br/>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= link_to "Sign in with #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider), class: "a", method: :post %><br />
  <% end %>
<% end %>
